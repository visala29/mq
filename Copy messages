pipeline {
    agent any
    stages {
        stage('Prepare Environment') {
            steps {
                bat '''
                if not exist C:\\output (
                    mkdir C:\\output
                )
                '''
            }
        }
        stage('Clear Message Queue') {
            steps {
                bat 'dmpmqmsg -m QM93WIN -I Q1 -r 1 -f C:\\output\\deleted-message-1.txt'
            }
        }
    }
}
